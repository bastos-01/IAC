.data
enter: .asciiz "\n"
fim:    .asciiz "\n FIM"
    .text
    .globl main
    
main:
	li $t0, 0xF0000000
	li $t1, 28

while:
	beq $t0, $0, end
	
	srl $t0, $t0, 4
	subi $t1, $t1, 4
	
	la $a0, enter
	li $v0, 4
	syscall
	
	move $a0, $t0
	
	li $v0, 1
	syscall
	
	j while
	
end:
	la $a0, fim
	li $v0, 4
	syscall
	